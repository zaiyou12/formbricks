{
  "extends": "@formbricks/tsconfig/nextjs.json",
  "include": ["."],
  "exclude": ["dist", "build", "node_modules"],
  "compilerOptions": {
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@prisma/client/*": ["@formbricks/database/client/*"],
    },
    "plugins": [
      {
        "name": "next",
      },
    ],
    "strictNullChecks": true,
  },
}
